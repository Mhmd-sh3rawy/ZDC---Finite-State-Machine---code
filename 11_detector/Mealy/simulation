#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d370fb21ea0 .scope module, "mealy_seq_tb" "mealy_seq_tb" 2 2;
 .timescale -9 -12;
v0x5d370fb374c0_0 .var "clk", 0 0;
v0x5d370fb37580_0 .var "rst_b", 0 0;
v0x5d370fb37650_0 .var "x", 0 0;
v0x5d370fb37750_0 .net "y", 0 0, L_0x5d370fb37970;  1 drivers
S_0x5d370fb22030 .scope module, "DUT" "mealy_seq" 2 7, 3 30 0, S_0x5d370fb21ea0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst_b";
    .port_info 3 /OUTPUT 1 "y";
P_0x5d370fb0e390 .param/l "S0" 1 3 37, C4<0>;
P_0x5d370fb0e3d0 .param/l "S1" 1 3 38, C4<1>;
L_0x7382a7cd0018 .functor BUFT 1, C4<1>, C4<0>, C4<0>, C4<0>;
L_0x5d370fb37850 .functor XNOR 1, v0x5d370fb36e80_0, L_0x7382a7cd0018, C4<0>, C4<0>;
L_0x5d370fb37970 .functor AND 1, L_0x5d370fb37850, v0x5d370fb37650_0, C4<1>, C4<1>;
v0x5d370fb0ff30_0 .var "NextState", 0 0;
v0x5d370fb36e80_0 .var "PresentState", 0 0;
v0x5d370fb36f40_0 .net/2u *"_ivl_0", 0 0, L_0x7382a7cd0018;  1 drivers
v0x5d370fb37030_0 .net *"_ivl_2", 0 0, L_0x5d370fb37850;  1 drivers
v0x5d370fb370f0_0 .net "clk", 0 0, v0x5d370fb374c0_0;  1 drivers
v0x5d370fb37200_0 .net "rst_b", 0 0, v0x5d370fb37580_0;  1 drivers
v0x5d370fb372c0_0 .net "x", 0 0, v0x5d370fb37650_0;  1 drivers
v0x5d370fb37380_0 .net "y", 0 0, L_0x5d370fb37970;  alias, 1 drivers
E_0x5d370fae76d0 .event edge, v0x5d370fb372c0_0, v0x5d370fb36e80_0;
E_0x5d370fae71f0/0 .event negedge, v0x5d370fb37200_0;
E_0x5d370fae71f0/1 .event posedge, v0x5d370fb370f0_0;
E_0x5d370fae71f0 .event/or E_0x5d370fae71f0/0, E_0x5d370fae71f0/1;
    .scope S_0x5d370fb22030;
T_0 ;
    %wait E_0x5d370fae71f0;
    %load/vec4 v0x5d370fb37200_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5d370fb36e80_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5d370fb0ff30_0;
    %assign/vec4 v0x5d370fb36e80_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5d370fb22030;
T_1 ;
    %wait E_0x5d370fae76d0;
    %load/vec4 v0x5d370fb36e80_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %jmp T_1.2;
T_1.0 ;
    %load/vec4 v0x5d370fb372c0_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.3, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_1.4, 8;
T_1.3 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_1.4, 8;
 ; End of false expr.
    %blend;
T_1.4;
    %store/vec4 v0x5d370fb0ff30_0, 0, 1;
    %jmp T_1.2;
T_1.1 ;
    %load/vec4 v0x5d370fb372c0_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.5, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_1.6, 8;
T_1.5 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_1.6, 8;
 ; End of false expr.
    %blend;
T_1.6;
    %store/vec4 v0x5d370fb0ff30_0, 0, 1;
    %jmp T_1.2;
T_1.2 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5d370fb21ea0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d370fb374c0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5d370fb374c0_0;
    %inv;
    %store/vec4 v0x5d370fb374c0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5d370fb21ea0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d370fb37580_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %delay 3000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d370fb37580_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d370fb37650_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5d370fb21ea0;
T_4 ;
    %vpi_call 2 35 "$dumpfile", "mealy_detector_11.vcd" {0 0 0};
    %vpi_call 2 36 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d370fb21ea0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
