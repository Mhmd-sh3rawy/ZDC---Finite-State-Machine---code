v0x638615e2aac0_0 .net "y", 0 0, L_0x638615e2abc0;  1 drivers
S_0x638615e156b0 .scope module, "DUT" "moore_seq" 2 7, 3 1 0, S_0x638615e15520;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst_b";
    .port_info 3 /OUTPUT 1 "y";
P_0x638615ddb620 .param/l "S0" 1 3 8, C4<00>;
P_0x638615ddb660 .param/l "S1" 1 3 9, C4<01>;
P_0x638615ddb6a0 .param/l "S2" 1 3 10, C4<10>;
v0x638615e02fa0_0 .var "NextState", 1 0;
v0x638615e2a290_0 .var "PresentState", 1 0;
L_0x7b2627868018 .functor BUFT 1, C4<10>, C4<0>, C4<0>, C4<0>;
v0x638615e2a370_0 .net/2u *"_ivl_0", 1 0, L_0x7b2627868018;  1 drivers
v0x638615e2a460_0 .net "clk", 0 0, v0x638615e2a830_0;  1 drivers
v0x638615e2a520_0 .net "rst_b", 0 0, v0x638615e2a8f0_0;  1 drivers
v0x638615e2a630_0 .net "x", 0 0, v0x638615e2a9c0_0;  1 drivers
v0x638615e2a6f0_0 .net "y", 0 0, L_0x638615e2abc0;  alias, 1 drivers
E_0x638615dda6d0 .event edge, v0x638615e2a290_0, v0x638615e2a630_0;
E_0x638615dda1f0/0 .event negedge, v0x638615e2a520_0;
E_0x638615dda1f0/1 .event posedge, v0x638615e2a460_0;
E_0x638615dda1f0 .event/or E_0x638615dda1f0/0, E_0x638615dda1f0/1;
L_0x638615e2abc0 .cmp/eq 2, v0x638615e2a290_0, L_0x7b2627868018;
    .scope S_0x638615e156b0;
T_0 ;
    %wait E_0x638615dda1f0;
    %load/vec4 v0x638615e2a520_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x638615e2a290_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x638615e02fa0_0;
    %assign/vec4 v0x638615e2a290_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x638615e156b0;
T_1 ;
    %wait E_0x638615dda6d0;
    %load/vec4 v0x638615e2a290_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %jmp T_1.3;
T_1.0 ;
    %load/vec4 v0x638615e2a630_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.4, 8;
    %pushi/vec4 1, 0, 2;
    %jmp/1 T_1.5, 8;
T_1.4 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_1.5, 8;
 ; End of false expr.
    %blend;
T_1.5;
    %store/vec4 v0x638615e02fa0_0, 0, 2;
    %jmp T_1.3;
T_1.1 ;
    %load/vec4 v0x638615e2a630_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.6, 8;
    %pushi/vec4 2, 0, 2;
    %jmp/1 T_1.7, 8;
T_1.6 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_1.7, 8;
 ; End of false expr.
    %blend;
T_1.7;
    %store/vec4 v0x638615e02fa0_0, 0, 2;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x638615e2a630_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.8, 8;
    %pushi/vec4 2, 0, 2;
    %jmp/1 T_1.9, 8;
T_1.8 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_1.9, 8;
 ; End of false expr.
    %blend;
T_1.9;
    %store/vec4 v0x638615e02fa0_0, 0, 2;
    %jmp T_1.3;
T_1.3 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x638615e15520;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638615e2a830_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x638615e2a830_0;
    %inv;
    %store/vec4 v0x638615e2a830_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x638615e15520;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638615e2a8f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638615e2a9c0_0, 0, 1;
    %delay 3000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638615e2a9c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x638615e2a8f0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x638615e2a9c0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638615e2a9c0_0, 0, 1;
    %delay 10000, 0;
